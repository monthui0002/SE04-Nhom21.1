package com.jkgh.jvm.parsing.types {
	
	
	public class ConstantPool {
		
		private var constantPool:Vector.<ConstantEntry>;
	
		public function ConstantPool(constantPool:Vector.<ConstantEntry>) {
			this.constantPool = constantPool;
		}
	
		public function getConstantAt(index:int):ConstantEntry {
			return constantPool[index];
		}
	
		public function getConstants():Vector.<ConstantEntry> {
			return constantPool;
		}
	}
}
