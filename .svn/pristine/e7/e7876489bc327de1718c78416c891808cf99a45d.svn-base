package com.jkgh.jvm.parsing.types {
	
	public class EnclosingMethodAttributeInfo {
	
		private var className:String;
		private var methodName:String;
		private var methodDescriptor:String;
	
		public function EnclosingMethodAttributeInfo(className:String, methodName:String, methodDescriptor:String) {
			this.className = className;
			this.methodName = methodName;
			this.methodDescriptor = methodDescriptor;
		}
	
		public function getClassName():String {
			return className;
		}
	
		public function getMethodName():String {
			return methodName;
		}
	
		public function getMethodDescriptor():String {
			return methodDescriptor;
		}
	
	}
}
