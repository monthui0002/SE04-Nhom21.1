package com.jkgh.jvm.runtime.execution {
	
	public class JVMSpecialInvocator extends JVMVirtualInvocator {
	
		private var ownerClass:JVMClass;
		
		public function JVMSpecialInvocator(identity:JVMMethodIdentity, ownerClass:JVMClass) {
			super(identity);
			this.ownerClass = ownerClass;	
		}
	
		public function getOwnerClass():JVMClass {
			return ownerClass;
		}
	
	}
}
